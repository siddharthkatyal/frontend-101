{

	"name": "My Extension",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This is a short description for our Chrome extension.",
	"browser_action": {
		"default_icon": "image.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}